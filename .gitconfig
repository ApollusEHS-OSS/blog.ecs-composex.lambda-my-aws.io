[gpg]
	program = /usr/bin/gpg
[credential]
	helper = !aws codecommit credential-helper $@
	UseHttpPath = true
[color]
  ui = auto

[color]
  ui = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan

